{
  "expo": {
    "name": "universe",
    "slug": "universe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/t.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/t.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/t.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rishabhk1.universe"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a29387d4-b606-4034-93d1-05ad98f9465b"
      }
    }
  }
}
